                                                        <option value="" disabled="disabled" selected="selected">Choose Fleet Type</option>
                                                        <t t-foreach="bus_types" t-as="bus_type">
                                                            <option t-att-value="bus_type.id">
                                                                <t t-esc="bus_type.name" />
                                                            </option>
                                                        </t>
                                                    </t>
                                                </select>
                                                <span class="fa fa-caret-down"></span>
                                            </div>
                                            <div class="col-lg-2 col-md-12 col-sm-12 col-12 text-center">
                                                <button type="submit" class="btn btn_search">Search</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </section>
                        <section id="ie_bus_booking_detail_table">
                            <div class="container">
                                <t t-foreach="search_ids" t-as="search_id">
                                    <div class="ie_bus_detail_box">
                                        <div class="row ms-0 me-0">
                                            <div class="col-lg-4 col-md-5 col-sm-12 col-xs-12">
                                                <div class="box">
                                                    <div class="ie_route_div d-flex">
                                                        <label class="ie_lbl w-20">Route : </label>
                                                        <span class="ie_val w-80"><t t-esc="search_id.route.baording_id.name" /> - <t t-esc="search_id.route.dropping_id.name" /></span>
                                                    </div>
                                                    <div class="ie_from_div d-flex">
                                                        <label class="ie_lbl w-20">From : </label>
                                                        <span class="ie_val w-80"><t t-esc="search_id.bording_from.name" /></span>
                                                    </div>
                                                    <div class="ie_from_div d-flex">
                                                        <label class="ie_lbl w-20">To : </label>
                                                        <span class="ie_val w-80"><t t-esc="search_id.to.name" /></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-4 col-md-3 col-sm-12 col-xs-12">
                                                <div class="box ie_daparture_box_div">
                                                    <div class="ie_departure_div d-flex">
                                                        <label class="ie_lbl w-28">Departure : </label>
                                                        <span class="ie_val w-72"><t t-esc="search_id.trip_start_date" t-options="{'widget': 'float_time'}" /></span>
                                                    </div>
                                                    <div class="ie_arrival_div d-flex">
                                                        <label class="ie_lbl w-28">Arrival : </label>
                                                        <span class="ie_val w-72"><t t-esc="search_id.trip_end_date" t-options="{'widget': 'float_time'}" /></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                                                <div class="box ie_total_seat_box_div">
                                                    <div class="ie_total_seat_div d-flex">
                                                        <label class="ie_lbl w-40">Total Seats : </label>
                                                        <span class="ie_val w-60"><t t-esc="search_id.total_seat" /></span>
                                                    </div>
                                                    <div class="ie_seat_booked_div d-flex">
                                                        <label class="ie_lbl w-40">Seat Booked : </label>
                                                        <span class="ie_val w-60"><t t-esc="search_id.seat_booked" /></span>
                                                    </div>
                                                    <div class="ie_remaining_seat_div d-flex">
                                                        <label class="ie_lbl w-40">Remaining Seats : </label>
                                                        <span class="ie_val w-60"><t t-esc="search_id.remaining_seats" /></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row ms-0 me-0 text-center">
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <div class="box pt-0">
                                                    <div class="ie_price_div mb-2">
                                                        <label class="ie_lbl">Price : </label>
                                                        <span class="ie_val"><t t-esc="search_id.price" /></span>
                                                    </div>
                                                    <div class="ie_book_btn_div">
                                                        <span class="ie_btn"><a href="#" class="btn btn-primary book_ticket" data-bs-toggle="modal"
                                                            t-att-data-bs-target="'#myModal%s00' %(search_id.id)" t-att-data-id="'%s' %(search_id.id)">Book</a>
                                                        <t t-call="ie_bus_web.seat_booking_template"></t></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </t>

                            </div>
                        </section>
                    </t>
                    <t t-else="">
                        <section id="ie_result_notfound_banner">
                            <div class="box">
                                <div class="container">
                                    <h3>No Result Found !</h3>
                                </div>
                            </div>
                        </section>
                    </t>
                </t>
                <div class="oe_structure oe_empty oe_structure_not_nearest" id="oe_structure_bus_booking_2" />
            </div>
        </t>
    </template>
</odoo>